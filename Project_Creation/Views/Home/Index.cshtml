        position: relative;
        z-index: 1;
        backdrop-filter: blur(5px);
        -webkit-backdrop-filter: blur(5px);
    }
    
    .hero-badge::before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: rgba(243, 153, 62, 0.05);
        border-radius: 50px;
        z-index: -1;
    }
    
    .hero-title {
        font-size: 4rem;
        font-weight: 800;
        margin-bottom: 1.5rem;
        line-height: 1.1;
        letter-spacing: -0.5px;
        background: linear-gradient(to right, #ffffff, #e0e0e0);
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        display: inline-block;
    }
    
    .hero-title span {
        background: linear-gradient(to right, #F3993E, #ffb366);
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        display: inline-block;
    }
    
    .hero-description {
        font-size: 1.15rem;
        line-height: 1.6;
        color: rgba(255, 255, 255, 1);
        margin-bottom: 2rem;
        max-width: 500px;
        text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
        font-weight: 500;
    }
    
    .hero-description .highlight {
        background: rgba(243, 153, 62, 0.15);
        padding: 8px 16px;
        border-radius: 8px;
        border-left: 3px solid #F3993E;
        display: inline-block;
        box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
        backdrop-filter: blur(5px);
        -webkit-backdrop-filter: blur(5px);
        color: white;
        font-weight: 600;
        letter-spacing: 0.3px;
        animation: highlight-glow 3s infinite alternate;
    }
    
    @@keyframes highlight-glow {
        0% { box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1); }
        100% { box-shadow: 0 8px 25px rgba(243, 153, 62, 0.25); }
    }
    
    /* Hero actions */
    .hero-actions {
        display: flex;
        gap: 1.5rem;
        flex-wrap: wrap;
        margin-top: 2.5rem;
    }
    
    .btn-learn, .btn-login {
        display: inline-flex;
        align-items: center;
        justify-content: center;
        padding: 14px 28px;
        border-radius: 50px;
        font-weight: 600;
        cursor: pointer;
        transition: all 0.3s ease;
        text-decoration: none;
        position: relative;
        z-index: 1;
        min-width: 160px;
        letter-spacing: 0.5px;
    }
    
    .btn-learn {
        background-color: rgba(255, 255, 255, 0.9);
        color: #304251;
        border: none;
        box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
    }
    
    .btn-learn:hover {
        background-color: #ffffff;
        transform: translateY(-3px);
        box-shadow: 0 8px 25px rgba(0, 0, 0, 0.15);
    }
    
    .btn-login {
        background: #F3993E;
        color: white;
        box-shadow: 0 4px 15px rgba(243, 153, 62, 0.3);
        border: none;
    }
    
    .btn-login:hover {
        background: #ff9f2d;
        transform: translateY(-3px);
        box-shadow: 0 8px 25px rgba(243, 153, 62, 0.4);
    }
    
    .btn-learn i, .btn-login i {
        margin-left: 10px;
        transition: transform 0.3s ease;
    }
    
    .btn-learn:hover i, .btn-login:hover i {
        transform: translateX(5px);
    }
    
    /* Dashboard visualization enhanced */
    .hero-visual-col {
        position: relative;
        z-index: 5;
    }
    
    .dashboard-visualization {
        width: 100%;
        background-color: rgba(22, 34, 46, 0.6); /* More transparent for glassmorphism */
        border-radius: 12px;
        overflow: hidden;
        border: 1px solid rgba(255, 255, 255, 0.1);
        box-shadow: 0 20px 50px rgba(0, 0, 0, 0.25);
        transform: perspective(1000px) rotateY(-5deg) rotateX(5deg);
        transition: all 0.5s ease;
        animation: float-slow 6s infinite ease-in-out;
        position: relative;
        backdrop-filter: blur(10px);
        -webkit-backdrop-filter: blur(10px);
    }
    
    .dashboard-visualization::after {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: linear-gradient(135deg, rgba(255, 255, 255, 0.1) 0%, rgba(255, 255, 255, 0) 100%);
        pointer-events: none;
        border-radius: 12px;
    }
    
    .dashboard-visualization:hover {
        transform: perspective(1000px) rotateY(0deg) rotateX(0deg) scale(1.02);
    }
    
    .dashboard-header {
        padding: 15px 20px;
        background-color: rgba(22, 28, 36, 0.9);
        display: flex;
        justify-content: space-between;
        align-items: center;
        border-bottom: 1px solid rgba(255, 255, 255, 0.1);
    }
    
    .dashboard-title {
        color: white;
        font-weight: 600;
        font-size: 1rem;
    }
    
    .dashboard-controls {
        display: flex;
        gap: 8px;
    }
    
    .control-dot {
        width: 10px;
        height: 10px;
        border-radius: 50%;
        background-color: rgba(255, 255, 255, 0.3);
    }
    
    .dashboard-content {
        padding: 20px;
        background-color: rgba(30, 42, 56, 0.8);
    }
    
    .dashboard-row {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
        gap: 15px;
        margin-bottom: 15px;
    }
    
    .dashboard-chart {
        background-color: rgba(22, 28, 36, 0.8);
        border-radius: 8px;
        overflow: hidden;
        height: 140px;
        border: 1px solid rgba(255, 255, 255, 0.05);
        box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
    }
    
    .chart-header {
        padding: 10px 15px;
        font-size: 0.85rem;
        color: white;
        font-weight: 500;
        background-color: rgba(15, 20, 25, 0.5);
        border-bottom: 1px solid rgba(255, 255, 255, 0.05);
    }
    
    .chart-content {
        height: calc(100% - 36px);
        padding: 10px;
    }
    
    .dashboard-metric {
        background-color: rgba(22, 28, 36, 0.8);
        border-radius: 8px;
        padding: 15px;
        display: flex;
        align-items: center;
        justify-content: space-between;
        border: 1px solid rgba(255, 255, 255, 0.05);
        box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
    }
    
    .metric-icon {
        width: 40px;
        height: 40px;
        border-radius: 8px;
        background: linear-gradient(135deg, rgba(243, 153, 62, 0.3) 0%, rgba(243, 153, 62, 0.1) 100%);
        color: #F3993E;
        display: flex;
        align-items: center;
        justify-content: center;
        font-size: 1.2rem;
    }
    
    .metric-data {
        flex-grow: 1;
        padding: 0 15px;
    }
    
    .metric-value {
        color: white;
        font-weight: 700;
        font-size: 1.3rem;
    }
    
    .metric-label {
        color: rgba(255, 255, 255, 0.6);
        font-size: 0.8rem;
    }
    
    .metric-trend {
        font-size: 0.8rem;
        font-weight: 600;
        padding: 4px 8px;
        border-radius: 4px;
        display: flex;
        align-items: center;
        gap: 5px;
    }
    
    .metric-trend.positive {
        background-color: rgba(52, 199, 89, 0.15);
        color: #4cff7a;
        border: 1px solid rgba(52, 199, 89, 0.2);
    }
    
    .metric-trend.negative {
        background-color: rgba(255, 69, 58, 0.15);
        color: #ff6b60;
        border: 1px solid rgba(255, 69, 58, 0.2);
    }
    
    /* Animation keyframes */
    @@keyframes float-slow {
        0%, 100% { transform: perspective(1000px) rotateY(-5deg) rotateX(5deg) translateY(0); }
        50% { transform: perspective(1000px) rotateY(-5deg) rotateX(5deg) translateY(-10px); }
    }
    
    @@keyframes float {
        0%, 100% { transform: translateY(0); }
        50% { transform: translateY(-8px); }
    }
    
    @@keyframes shine {
        0% { left: -100%; }
        20%, 100% { left: 100%; }
    }
    
