<script>
    // Animation on scroll
    document.addEventListener('DOMContentLoaded', function() {
        // Counter animation
        const counters = document.querySelectorAll('.counter');
        counters.forEach(counter => {
            const target = +counter.innerText;
            const duration = 2000;
            let startTime = null;
            
            function updateCounter(timestamp) {
                if (!startTime) startTime = timestamp;
                const progress = timestamp - startTime;
                const value = Math.floor((progress / duration) * target);
                
                counter.innerText = Math.min(value, target);
                
                if (progress < duration) {
                    requestAnimationFrame(updateCounter);
                }
            }
            
            requestAnimationFrame(updateCounter);
        });
        
        // Scroll animations
        function checkScrollAnimation() {
            const sections = document.querySelectorAll('.section-header, .feature-card, .growth-image, .growth-content, .growth-point, .cta-title, .cta-text, .cta-button');
            
            sections.forEach(section => {
                const sectionTop = section.getBoundingClientRect().top;
                if (sectionTop < window.innerHeight * 0.8) {
                    section.classList.add('animated');
                }
            });
        }
        
        window.addEventListener('scroll', checkScrollAnimation);
        checkScrollAnimation(); // Check on load
        
        // Login loading animation
        const loginButtons = document.querySelectorAll('.login-trigger');
        const loadingOverlay = document.querySelector('.login-loading-overlay');
        
        loginButtons.forEach(button => {
            button.addEventListener('click', function(e) {
                e.preventDefault();
                
                // Show loading overlay
                loadingOverlay.classList.add('active');
                
                // Redirect after a short delay (simulating loading)
                setTimeout(() => {
                    window.location.href = this.getAttribute('href');
                }, 1500);
            });
        });
    });
</script>

