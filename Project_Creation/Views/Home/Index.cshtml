            <path class="shape shape-2" d="M1201.8,748.4c-32.9,43.6-191.3,78.7-259.9,49.2s-96.1-126.8-178.1-166.1c-82-39.3-203.2-18.1-250.7,25.7s-90.7,139.3-175.3,166.1C253.2,850.2,87.7,848.5,57.3,737S156.5,450.6,276.9,429s233.2-44.8,286.5-69.8s106-112,77.4-186.4" fill="none" stroke="rgba(255, 255, 255, 0.05)" stroke-width="2" />
            
