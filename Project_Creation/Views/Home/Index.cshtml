                            <!-- Data Points -->
                            <circle cx="90" cy="280" r="5" fill="#F3993E"/>
                            <circle cx="150" cy="220" r="5" fill="#F3993E">
                                <animate attributeName="cy" dur="4s" repeatCount="indefinite" values="220;240;220"/>
                            </circle>
                            <circle cx="210" cy="260" r="5" fill="#F3993E">
                                <animate attributeName="cy" dur="4s" repeatCount="indefinite" values="260;230;260"/>
                            </circle>
                            <circle cx="270" cy="180" r="5" fill="#F3993E">
                                <animate attributeName="cy" dur="4s" repeatCount="indefinite" values="180;160;180"/>
                            </circle>
                            <circle cx="330" cy="200" r="5" fill="#F3993E">
                                <animate attributeName="cy" dur="4s" repeatCount="indefinite" values="200;220;200"/>
                            </circle>
                            <circle cx="390" cy="150" r="5" fill="#F3993E">
                                <animate attributeName="cy" dur="4s" repeatCount="indefinite" values="150;130;150"/>
                            </circle>
                            <circle cx="450" cy="190" r="5" fill="#F3993E">
                                <animate attributeName="cy" dur="4s" repeatCount="indefinite" values="190;170;190"/>
                            </circle>
                            
                            <!-- Metrics Cards -->
                            <rect x="490" y="130" width="240" height="80" rx="5" fill="white" stroke="#e9ecef" stroke-width="1"/>
                            <rect x="490" y="230" width="240" height="80" rx="5" fill="white" stroke="#e9ecef" stroke-width="1"/>
                            <rect x="490" y="330" width="240" height="80" rx="5" fill="white" stroke="#e9ecef" stroke-width="1"/>
                            
                            <!-- Metrics Icons -->
                            <circle cx="520" cy="170" r="15" fill="rgba(243, 153, 62, 0.2)"/>
                            <circle cx="520" cy="270" r="15" fill="rgba(52, 152, 219, 0.2)"/>
                            <circle cx="520" cy="370" r="15" fill="rgba(46, 204, 113, 0.2)"/>
                        </svg>
                    </div>
                    
                    
                    <!-- Stat indicators -->
                    <div class="growth-stats">
                        <div class="growth-stat-item">
                            <div class="growth-stat-value">+94<span>%</span></div>
                            <div class="growth-stat-label">Conversion Rate</div>
                        </div>
                        <div class="growth-stat-item">
                            <div class="growth-stat-value">4.7<span>x</span></div>
                            <div class="growth-stat-label">Efficiency</div>
                        </div>
                    </div>
                </div>
            </div>
            
            <div class="col-lg-6 growth-content">
                <div class="growth-content-inner">
                    <span class="growth-badge">Propel Your Business</span>
                    <h2 class="growth-title">Accelerate Your <span>Business Growth</span></h2>
                    <p class="growth-description">GrowTrack is designed to help small and medium enterprises gain actionable insights, identify growth opportunities, and make strategic decisions based on real-time data.</p>
                    
                    <div class="growth-points">
                        <div class="growth-point-item">
                            <div class="growth-point-icon">
                                <i class="fas fa-chart-line"></i>
                            </div>
                            <div class="growth-point-content">
                                <h5 class="growth-point-title">Identify Growth Opportunities</h5>
                                <p class="growth-point-desc">Discover untapped markets and opportunities through advanced data analytics and trend analysis.</p>
                            </div>
                        </div>
                
                        <div class="growth-point-item">
                            <div class="growth-point-icon">
                                <i class="fas fa-chart-bar"></i>
                            </div>
                            <div class="growth-point-content">
                                <h5 class="growth-point-title">Track Business Performance</h5>
                                <p class="growth-point-desc">Monitor key performance indicators with customizable dashboards that highlight areas of success and improvement.</p>
                            </div>
                        </div>
                        
                        <div class="growth-point-item">
                            <div class="growth-point-icon">
                                <i class="fas fa-chess-knight"></i>
                            </div>
                            <div class="growth-point-content">
                                <h5 class="growth-point-title">Strategic Planning</h5>
                                <p class="growth-point-desc">Develop effective business strategies based on predictive analytics and growth forecasting models.</p>
                            </div>
                        </div>
                    </div>

                    <a href="#" class="growth-cta">
                        <span>Start Growing Today</span>
                        <i class="fas fa-arrow-right"></i>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>



<!-- Enhanced CTA Section -->
<div class="cta-section text-white vh-100 d-flex align-items-center">
    <!-- Background elements -->
    <div class="cta-bg">
        <div class="cta-circle cta-circle-1"></div>
        <div class="cta-circle cta-circle-2"></div>
        <div class="cta-pattern"></div>
        
