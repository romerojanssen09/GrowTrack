@{
    ViewData["Title"] = "Home - GrowTrack";
}

<!-- Include the auth navbar -->
<partial name="_AuthNavbar" />

<!-- Redesigned Hero Section with Enhanced Background -->
<div class="hero-section position-relative vh-100 d-flex align-items-center">
    <!-- Modern background elements with image -->
    <div class="hero-bg">
        <div class="hero-bg-image"></div>
        <div class="glassmorphism-overlay"></div>
        <div class="gradient-sphere gradient-1"></div>
        <div class="gradient-sphere gradient-2"></div>
        <div class="gradient-sphere gradient-3"></div>
        <div class="animated-circle circle-1"></div>
        <div class="animated-circle circle-2"></div>
        <div class="animated-circle circle-3"></div>
        <div class="animated-circle circle-4"></div>
        <div class="animated-circle circle-5"></div>
        <div class="animated-circle circle-6"></div>
        <div class="animated-circle circle-7"></div>
        <div class="animated-circle circle-8"></div>
        <div class="animated-circle circle-9"></div>
        <div class="gradient-overlay"></div>
        <div class="grid-pattern"></div>
    </div>
    
    <div class="container position-relative">
        <div class="row hero-row">
            <!-- Left Content Column -->
            <div class="col-lg-5 hero-content-col">
                <div class="hero-badge">
                    <span>BUSINESS INTELLIGENCE</span>
                </div>
                <h1 class="hero-title">Grow<span>Track</span></h1>
                    <p class="hero-description">
                        Powerful analytics and business intelligence for data-driven companies. Transform your business decisions with actionable insights.
                    </p>
                <div class="hero-actions">
                    <a href="#features" class="btn-learn">
                        <span>Learn More</span>
                        <i class="fas fa-arrow-right"></i>
                    </a>
                    <a asp-controller="Login" asp-action="Login" class="btn-login login-trigger">
                        <span class="btn-text">Login Now</span>
                        <i class="fas fa-sign-in-alt"></i>
                    </a>
                </div>
            </div>
            
            <!-- Right Dashboard Visualization Column -->
            <div class="col-lg-7 hero-visual-col">
                <div class="dashboard-visualization">
                    <!-- Dashboard Header -->
                    <div class="dashboard-header">
                        <div class="dashboard-title">Business Analytics Dashboard</div>
                        <div class="dashboard-controls">
                            <span class="control-dot"></span>
                            <span class="control-dot"></span>
                            <span class="control-dot"></span>
                        </div>
                    </div>
                    
                    <!-- Dashboard Content -->
                    <div class="dashboard-content">
                        <!-- Charts Row -->
                        <div class="dashboard-row">
                            <div class="dashboard-chart line-chart">
                                <div class="chart-header">
                                    <span>Revenue Growth</span>
                                </div>
                                <div class="chart-content">
                                    <svg viewBox="0 0 100 30" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M0,25 L10,20 L20,22 L30,15 L40,18 L50,10 L60,12 L70,8 L80,5 L90,3 L100,2" fill="none" stroke="#F3993E" stroke-width="1.5" />
                                        <path d="M0,25 L10,20 L20,22 L30,15 L40,18 L50,10 L60,12 L70,8 L80,5 L90,3 L100,2" fill="url(#lineGradient)" stroke="none" opacity="0.3" />
                                        <defs>
                                            <linearGradient id="lineGradient" x1="0%" y1="0%" x2="0%" y2="100%">
                                                <stop offset="0%" stop-color="#F3993E" />
                                                <stop offset="100%" stop-color="#F3993E" stop-opacity="0" />
                                            </linearGradient>
                                        </defs>
                                    </svg>
                                </div>
                            </div>
                            <div class="dashboard-chart bar-chart">
                                <div class="chart-header">
                                    <span>Monthly Sales</span>
                                </div>
                                <div class="chart-content">
                                    <svg viewBox="0 0 100 30" xmlns="http://www.w3.org/2000/svg">
                                        <rect x="5" y="20" width="5" height="10" fill="#304251" />
                                        <rect x="15" y="15" width="5" height="15" fill="#304251" />
                                        <rect x="25" y="18" width="5" height="12" fill="#304251" />
                                        <rect x="35" y="12" width="5" height="18" fill="#F3993E" />
                                        <rect x="45" y="8" width="5" height="22" fill="#F3993E" />
                                        <rect x="55" y="5" width="5" height="25" fill="#F3993E" />
                                        <rect x="65" y="10" width="5" height="20" fill="#304251" />
                                        <rect x="75" y="15" width="5" height="15" fill="#304251" />
                                        <rect x="85" y="7" width="5" height="23" fill="#F3993E" />
                                    </svg>
                                </div>
                            </div>
                        </div>
                        
                        <!-- Metrics Row -->
                        <div class="dashboard-row">
                            <div class="dashboard-metric">
                                <div class="metric-icon">
                                    <i class="fas fa-users"></i>
                                </div>
                                <div class="metric-data">
                                    <div class="metric-value">2,845</div>
                                    <div class="metric-label">Active Users</div>
                                </div>
                                <div class="metric-trend positive">
                                    <i class="fas fa-arrow-up"></i>
                                    <span>12%</span>
                                </div>
                            </div>
                            <div class="dashboard-metric">
                                <div class="metric-icon">
                                    <i class="fas fa-chart-pie"></i>
                                </div>
                                <div class="metric-data">
                                    <div class="metric-value">$42K</div>
                                    <div class="metric-label">Monthly Revenue</div>
                                </div>
                                <div class="metric-trend positive">
                                    <i class="fas fa-arrow-up"></i>
                                    <span>8%</span>
                                </div>
                            </div>
                            <div class="dashboard-metric">
                                <div class="metric-icon">
                                    <i class="fas fa-chart-line"></i>
                                </div>
                                <div class="metric-data">
                                    <div class="metric-value">89%</div>
                                    <div class="metric-label">Growth Rate</div>
                                </div>
                                <div class="metric-trend positive">
                                    <i class="fas fa-arrow-up"></i>
                                    <span>5%</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
    <div class="wave-bottom">
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1440 100" preserveAspectRatio="none">
            <rect x="0" y="50" width="1440" height="50" fill="#F7F5EF"/>
        </svg>
    </div>
</div>

<!-- Features Section -->
<div id="features" class="features-section d-flex align-items-center py-5">
    <!-- Decorative elements -->
    <div class="features-decorations">
        <div class="dec-circle dec-circle-1"></div>
        <div class="dec-circle dec-circle-2"></div>
        <div class="dec-line dec-line-1"></div>
        <div class="dec-line dec-line-2"></div>
        <div class="dec-dots"></div>
        
        <!-- Animated Glowing Circles -->
        <div class="feature-glow-circle circle-orange-lg"></div>
        <div class="feature-glow-circle circle-blue-md"></div>
        <div class="feature-glow-circle circle-orange-sm"></div>
        <div class="feature-glow-circle circle-blue-lg"></div>
        <div class="feature-glow-circle circle-orange-md"></div>
        
        <!-- Connecting animated line -->
        <div class="connecting-line features-to-growth">
            <svg viewBox="0 0 100 100" preserveAspectRatio="none">
                <path class="path-line" d="M10,90 Q30,70 50,80 T90,50" stroke="#F3993E" stroke-width="0.5" fill="none" stroke-dasharray="5,3" />
                <circle class="path-dot" cx="10" cy="90" r="1" fill="#F3993E" />
                <circle class="path-dot" cx="90" cy="50" r="1" fill="#F3993E" />
            </svg>
        </div>
    </div>
    
    <!-- Floating particles -->
    <div class="particles-container">
        <div class="particle particle-1"></div>
        <div class="particle particle-2"></div>
        <div class="particle particle-3"></div>
        <div class="particle particle-4"></div>
        <div class="particle particle-5"></div>
        <div class="particle particle-6"></div>
    </div>
        
    <div class="container position-relative">
        <div class="text-center mb-4 mb-lg-5 section-header">
            <span class="section-badge">What We Offer</span>
            <h2 class="section-title animate-title">Key Features</h2>
            <p class="section-subtitle">Powerful analytics tools to help you understand and grow your business</p>
        </div>
        
        <div class="row g-4 features-container">
            <div class="col-lg-4 col-md-6 col-sm-12 feature-item">
                <div class="feature-card h-100">
                    <div class="feature-card-inner">
                        <div class="feature-icon-wrapper">
                            <div class="feature-icon-bg"></div>
                            <div class="feature-icon" style="background-color: rgba(243, 153, 62, 0.1); color: #F3993E;">
                                <i class="fas fa-users"></i>
                            </div>
                        </div>
                        <h3 class="feature-title">Business Analytics</h3>
                        <p class="feature-desc">Get comprehensive insights into your business performance with customizable dashboards and real-time analytics.</p>
                        <div class="feature-indicator"></div>
                    </div>
                    <a href="#" class="feature-link">
                        <span>Learn More</span>
                        <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M14.1667 7.5L17.5 10.8333M17.5 10.8333L14.1667 14.1667M17.5 10.8333H2.5" stroke="#F3993E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        </svg>
                    </a>
                </div>
            </div>
            
            <div class="col-lg-4 col-md-6 col-sm-12 feature-item">
                <div class="feature-card h-100">
                    <div class="feature-card-inner">
                        <div class="feature-icon-wrapper">
                            <div class="feature-icon-bg"></div>
                            <div class="feature-icon" style="background-color: rgba(48, 66, 81, 0.1); color: #304251;">
                                <i class="fas fa-chart-line"></i>
                            </div>
                        </div>
                        <h3 class="feature-title">Performance Tracking</h3>
                        <p class="feature-desc">Track key performance indicators and growth metrics with interactive visualizations that help you make data-driven business decisions.</p>
                        <div class="feature-indicator"></div>
                    </div>
                    <a href="#" class="feature-link">
                        <span>Learn More</span>
                        <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M14.1667 7.5L17.5 10.8333M17.5 10.8333L14.1667 14.1667M17.5 10.8333H2.5" stroke="#304251" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        </svg>
                    </a>
                </div>
            </div>
            
            <div class="col-lg-4 col-md-6 col-sm-12 feature-item mx-auto">
                <div class="feature-card h-100">
                    <div class="feature-card-inner">
                        <div class="feature-icon-wrapper">
                            <div class="feature-icon-bg"></div>
                            <div class="feature-icon" style="background-color: rgba(233, 243, 245, 0.6); color: #304251;">
                                <i class="fas fa-tasks"></i>
                            </div>
                        </div>
                        <h3 class="feature-title">Growth Forecasting</h3>
                        <p class="feature-desc">Leverage predictive analytics to forecast business growth, identify opportunities, and plan strategic initiatives with confidence.</p>
                        <div class="feature-indicator"></div>
                    </div>
                    <a href="#" class="feature-link">
                        <span>Learn More</span>
                        <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <path d="M14.1667 7.5L17.5 10.8333M17.5 10.8333L14.1667 14.1667M17.5 10.8333H2.5" stroke="#304251" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                        </svg>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Growth Section -->
<div class="growth-section d-flex align-items-center py-5">
    <!-- Background elements -->
    <div class="growth-bg">
        <div class="growth-circle growth-circle-1"></div>
        <div class="growth-circle growth-circle-2"></div>
        <div class="growth-dots"></div>
        
        <!-- Connecting animated line -->
        <div class="connecting-line growth-to-cta">
            <svg viewBox="0 0 100 100" preserveAspectRatio="none">
                <path class="path-line" d="M10,20 Q30,40 50,30 T90,70" stroke="#304251" stroke-width="0.5" fill="none" stroke-dasharray="5,3" />
                <circle class="path-dot" cx="10" cy="20" r="1" fill="#304251" />
                <circle class="path-dot" cx="90" cy="70" r="1" fill="#304251" />
            </svg>
        </div>
    </div>
    
    <div class="container position-relative">
        <div class="row align-items-center">
            <div class="col-lg-6 mb-5 mb-lg-0 growth-visual">
                <div class="growth-visual-wrapper">
                    <!-- Business analytics dashboard visualization -->
                    <div class="dashboard-animation">
                        <svg class="analytics-svg" viewBox="0 0 800 500" xmlns="http://www.w3.org/2000/svg">
                            <!-- Dashboard Frame -->
                            <rect x="50" y="50" width="700" height="400" rx="10" fill="#f8f9fa" stroke="#e9ecef" stroke-width="2"/>
                            
                            <!-- Header Bar -->
                            <rect x="50" y="50" width="700" height="60" rx="10" fill="#304251" stroke="none"/>
                            
                            <!-- Chart Area -->
                            <rect x="70" y="130" width="400" height="200" rx="5" fill="white" stroke="#e9ecef" stroke-width="1"/>
                            
                            <!-- Line Chart -->
                            <polyline points="90,280 150,220 210,260 270,180 330,200 390,150 450,190" fill="none" stroke="#F3993E" stroke-width="3">
                                <animate attributeName="points" dur="4s" repeatCount="indefinite" 
                                    values="90,280 150,220 210,260 270,180 330,200 390,150 450,190;
                                           90,260 150,240 210,230 270,160 330,220 390,130 450,170;
                                           90,280 150,220 210,260 270,180 330,200 390,150 450,190"/>
